year <- sub("-.*", "", meta$Date)
note <- sprintf("{R} package version %s", meta$Version)

citEntry(
  entry = "Misc",
  title = "{meanr}: Basic Sentiment Analysis Scorer",
  author = person("Drew", "Schmidt"),
  year = year,
  note = note,
  url = "https://cran.r-project.org/package=meanr",
  textVersion = NULL,
  key = "meanr2017"
)
